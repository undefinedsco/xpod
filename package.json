{
  "name": "xpod",
  "version": "0.1.0",
  "description": "Xpod is an extended Community Solid Server, offering rich-feature, production-level Solid Pod and identity management.",
  "repository": "https://github.com/undefinedsco/xpod",
  "author": "developer@undefineds.co",
  "license": "MIT",
  "private": false,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "lsd:module": "https://linkedsoftwaredependencies.org/bundles/npm/@undefineds/xpod",
  "lsd:components": "dist/components/components.jsonld",
  "lsd:contexts": {
    "https://linkedsoftwaredependencies.org/bundles/npm/@undefineds/xpod/^0.0.0/components/context.jsonld": "dist/components/context.jsonld"
  },
  "lsd:importPaths": {
    "https://linkedsoftwaredependencies.org/bundles/npm/@undefineds/xpod/^0.0.0/components/": "dist/components/",
    "https://linkedsoftwaredependencies.org/bundles/npm/@undefineds/xpod/^0.0.0/config/": "config/",
    "https://linkedsoftwaredependencies.org/bundles/npm/@undefineds/xpod/^0.0.0/dist/": "dist/",
    "https://linkedsoftwaredependencies.org/bundles/npm/@undefineds/xpod/^0.0.0/static/": "static/",
    "https://linkedsoftwaredependencies.org/bundles/npm/@undefineds/xpod/^0.0.0/templates/": "templates/"
  },
  "scripts": {
    "build": "npm run build:ts && npm run build:components && npm run build:components:fix && npm run build:ui",
    "build:ts": "tsc",
    "build:components": "componentsjs-generator -s src -c dist/components -r undefineds",
    "build:components:fix": "cp components/*.jsonld dist/storage/quint/ 2>/dev/null; cp components/VectorStore.jsonld dist/storage/vector/ 2>/dev/null; cp components/ProviderRegistry.jsonld components/EmbeddingService.jsonld components/CredentialReader.jsonld dist/embedding/ 2>/dev/null; node -e \"const fs=require('fs');const f='dist/components/components.jsonld';const c=JSON.parse(fs.readFileSync(f));const imports=['npmd:@undefineds/xpod/^0.0.0/dist/storage/quint/types.jsonld','npmd:@undefineds/xpod/^0.0.0/dist/storage/quint/BaseQuintStore.jsonld','npmd:@undefineds/xpod/^0.0.0/dist/storage/vector/VectorStore.jsonld','npmd:@undefineds/xpod/^0.0.0/dist/embedding/ProviderRegistry.jsonld','npmd:@undefineds/xpod/^0.0.0/dist/embedding/EmbeddingService.jsonld','npmd:@undefineds/xpod/^0.0.0/dist/embedding/CredentialReader.jsonld'];imports.forEach(i=>{if(!c.import.includes(i)){c.import.splice(-2,0,i)}});fs.writeFileSync(f,JSON.stringify(c,null,2))\"",
    "build:ui": "cd ui && npm run build",
    "prepare.": "yarn run build",
    "clean": "rm -rf data/*",
    "clean:test": "rm -rf .test-data && rm -f *.sqlite *.sqlite-wal *.sqlite-shm",
    "vanilla": "npx community-solid-server -c config/main.vanilla.json -f ./data_vanilla -p 3000",
    "local": "dotenv -e .env.local -- community-solid-server -c config/main.local.json config/extensions.local.json -m .",
    "server": "dotenv -e .env.server -- community-solid-server -c config/main.server.json config/extensions.server.json -m .",
    "dev:css": "dotenv -e .env.local -- community-solid-server -c config/main.local.json config/extensions.local.json -m .",
    "watch:css": "nodemon --watch config --watch templates --watch static --watch src --ext json,ejs,html,ts,tsx --exec \"yarn build:ts && yarn build:components && yarn dev:css\"",
    "dev:css:dev": "dotenv -e .env.local -- community-solid-server -c config/main.dev.json config/extensions.dev.json -m .",
    "watch:css:dev": "nodemon --watch config --watch templates --watch static --watch src --ext json,ejs,html,ts,tsx --exec \"yarn build:ts && yarn build:components && yarn dev:css:dev\"",
    "dev:css:server": "dotenv -e .env.server -- community-solid-server -c config/main.server.json config/extensions.server.json -m .",
    "watch:css:server": "nodemon --watch config --watch templates --watch static --watch src --ext json,ejs,html,ts,tsx --exec \"yarn build:ts && yarn build:components && yarn dev:css:server\"",
    "cluster:center": "dotenv -e .env.cluster -- community-solid-server -c config/main.server.json config/extensions.cluster.router.json -m . -p 3100",
    "cluster:edge": "dotenv -e .env.edge -- community-solid-server -c config/main.local.json config/extensions.local.json -m .",
    "test": "vitest --run",
    "test:run": "vitest --run",
    "test:terminal": "npx ts-node scripts/test-terminal.ts",
    "test:coverage": "vitest --run --coverage",
    "test:integration": "XPOD_RUN_INTEGRATION_TESTS=true XPOD_RUN_SERVER_INTEGRATION=true CSS_SEED_CONFIG=$PWD/config/seeds/test.json dotenv -e .env.local -- vitest --run",
    "test:cluster": "XPOD_RUN_CLUSTER_TESTS=true vitest --run tests/integration/MultiNodeCluster.integration.test.ts",
    "test:all": "XPOD_RUN_INTEGRATION_TESTS=true XPOD_RUN_SERVER_INTEGRATION=true vitest --run --coverage",
    "test:quad-vs-quint": "vitest --run tests/storage/quad-vs-quint-comparison.test.ts",
    "test:w3c": "vitest --run tests/storage/w3c-sparql-full.test.ts",
    "test:storage": "vitest --run tests/storage/"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@solid/community-server": "^8.0.0-alpha.1",
    "@types/better-sqlite3": "^7.6.13",
    "@types/http-proxy": "^1.17.17",
    "@types/json-stable-stringify": "^1.2.0",
    "@types/pg": "^8.10.2",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.4.0",
    "autoprefixer": "^10.4.17",
    "componentsjs-generator": "^4.3.0",
    "dotenv": "^16.4.5",
    "dotenv-cli": "^11.0.0",
    "drizzle-kit": "^0.31.8",
    "jsdom": "^24.0.0",
    "jsonld": "^8.3.3",
    "memory-level": "^3.1.0",
    "node-pty": "^1.1.0",
    "nodemon": "^3.1.10",
    "postcss": "^8.4.35",
    "rdf-test-suite": "^2.1.3",
    "tailwindcss": "^3.4.3",
    "ts-node": "^10.9.2",
    "typescript": "5.5",
    "vite": "^5.1.6",
    "vitest": "^1.4.0"
  },
  "dependencies": {
    "@ai-sdk/openai": "^3.0.2",
    "@comunica/query-sparql-rdfjs": "^4.5.0",
    "@electric-sql/pglite": "^0.3.14",
    "@inrupt/solid-client-authn-node": "^3.1.0",
    "@types/yargs": "^17.0.35",
    "abstract-level": "^1.0.4",
    "acme-client": "^5.1.3",
    "ai": "^6.0.6",
    "better-sqlite3": "^12.5.0",
    "classic-level": "^1.4.1",
    "clsx": "^2.1.0",
    "drizzle-orm": "^0.30.10",
    "drizzle-solid": "0.2.1",
    "http-proxy": "^1.18.1",
    "i18next": "^23.10.1",
    "knex": "^3.1.0",
    "level-supports": "^4.0.1",
    "minio": "^8.0.2",
    "pg": "^8.13.1",
    "pg-query-stream": "^4.7.1",
    "pgvector": "^0.2.1",
    "portfinder": "^1.0.38",
    "quadstore": "^15.4.1",
    "quadstore-comunica": "^6.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^13.3.1",
    "react-router-dom": "^6.22.3",
    "sqlite-vec": "^0.1.7-alpha.2",
    "sqlite3": "^5.1.7",
    "tree-kill": "^1.2.2",
    "winston-daily-rotate-file": "^5.0.0",
    "yargs": "^18.0.0",
    "zod": "^4.3.2"
  }
}
