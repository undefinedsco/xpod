{
  "name": "@undefineds.co/xpod",
  "version": "0.1.0",
  "description": "Xpod is an extended Community Solid Server, offering rich-feature, production-level Solid Pod and identity management.",
  "repository": "https://github.com/undefinedsco/xpod",
  "author": "developer@undefineds.co",
  "license": "MIT",
  "private": false,
  "bin": {
    "xpod": "./dist/main.js",
    "xpod-single": "./dist/xpod.single.cjs"
  },
  "files": [
    "dist",
    "config",
    "components",
    "templates",
    "static"
  ],
  "engines": {
    "node": ">=22 <23"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "lsd:module": "https://linkedsoftwaredependencies.org/bundles/npm/@undefineds.co/xpod",
  "lsd:components": "dist/components/components.jsonld",
  "lsd:contexts": {
    "https://linkedsoftwaredependencies.org/bundles/npm/@undefineds.co/xpod/^0.0.0/components/context.jsonld": "dist/components/context.jsonld"
  },
  "lsd:importPaths": {
    "https://linkedsoftwaredependencies.org/bundles/npm/@undefineds.co/xpod/^0.0.0/components/": "dist/components/",
    "https://linkedsoftwaredependencies.org/bundles/npm/@undefineds.co/xpod/^0.0.0/config/": "config/",
    "https://linkedsoftwaredependencies.org/bundles/npm/@undefineds.co/xpod/^0.0.0/dist/": "dist/",
    "https://linkedsoftwaredependencies.org/bundles/npm/@undefineds.co/xpod/^0.0.0/static/": "static/",
    "https://linkedsoftwaredependencies.org/bundles/npm/@undefineds.co/xpod/^0.0.0/templates/": "templates/"
  },
  "scripts": {
    "build": "yarn build:ts && yarn build:components && yarn build:ui",
    "build:ts": "tsc",
    "build:components": "componentsjs-generator -s src -c dist/components -r undefineds",
    "build:ui": "cd ui && yarn build",
    "release": "yarn build && npm publish --access public",
    "prepare.": "yarn build",
    "clean": "rm -rf data/*",
    "clean:test": "rm -rf .test-data && rm -f *.sqlite *.sqlite-wal *.sqlite-shm",
    "vanilla": "yarn community-solid-server -c config/main.vanilla.json -f ./data_vanilla -p 3000",
    "start": "yarn ts-node src/main.ts",
    "local": "dotenv -e .env.local -- yarn start -c config/local.json",
    "cloud": "dotenv -e .env.cloud -- yarn start -c config/cloud.json",
    "dev": "./scripts/dev-start.sh",
    "dev:local": "./scripts/dev-start.sh --local",
    "dev:seed": "NODE_ENV=development CSS_SEED_CONFIG=$PWD/config/seed.dev.json dotenv -e .env.local -- yarn start -c config/local.json",
    "dev:cloud": "NODE_ENV=development CSS_SEED_CONFIG=$PWD/config/seed.dev.json dotenv -e .env.cloud -- yarn start -c config/cloud.json",
    "dev:api": "NODE_ENV=development dotenv -e .env.local -- yarn ts-node src/api/main.ts",
    "dev:init": "NODE_ENV=development yarn ts-node scripts/init-dev-credentials.ts",
    "dev:test": "NODE_ENV=development yarn ts-node scripts/test-dev-flow.ts",
    "dev:cluster": "docker-compose -f docker-compose.cluster.yml up --build",
    "dev:cluster:down": "docker-compose -f docker-compose.cluster.yml down",
    "dev:css": "dotenv -e .env.local -- community-solid-server -c config/local.json -m .",
    "watch:css": "nodemon --watch config --watch templates --watch static --watch src --ext json,ejs,html,ts,tsx --exec \"yarn build:ts && yarn build:components && yarn dev:css\"",
    "test": "vitest --run",
    "test:run": "vitest --run",
    "test:terminal": "npx ts-node scripts/test-terminal.ts",
    "test:coverage": "vitest --run --coverage",
    "test:integration": "XPOD_RUN_INTEGRATION_TESTS=true CSS_SEED_CONFIG=$PWD/config/seeds/test.json dotenv -e .env.local -- vitest --run",
    "test:cluster": "XPOD_RUN_CLUSTER_TESTS=true vitest --run tests/integration/MultiNodeCluster.integration.test.ts",
    "test:docker": "yarn test:docker:lite",
    "test:all": "XPOD_RUN_INTEGRATION_TESTS=true vitest --run --coverage",
    "test:quad-vs-quint": "vitest --run tests/storage/quad-vs-quint-comparison.test.ts",
    "test:w3c": "vitest --run tests/storage/w3c-sparql-full.test.ts",
    "test:storage": "vitest --run tests/storage/",
    "dev:cluster:lite": "docker-compose -f docker-compose.standalone.yml up --build",
    "dev:cluster:lite:down": "docker-compose -f docker-compose.standalone.yml down",
    "test:docker:lite": "XPOD_RUN_DOCKER_LITE_TESTS=true vitest --run tests/integration/AiHandler.integration.test.ts tests/integration/ApiServer.integration.test.ts tests/integration/DockerStandalone.integration.test.ts tests/integration/ServerModeRoot.test.ts tests/integration/notification-subscription.test.ts tests/integration/SignalHandler.integration.test.ts tests/integration/FrpTunnel.integration.test.ts tests/integration/DockerCluster.integration.test.ts tests/integration/EdgeClusterFlow.test.ts tests/integration/MultiNodeCluster.integration.test.ts",
    "test:docker:full": "XPOD_RUN_DOCKER_LITE_TESTS=true XPOD_RUN_DOCKER_TESTS=true vitest --run tests/integration/AiHandler.integration.test.ts tests/integration/ApiServer.integration.test.ts tests/integration/DockerStandalone.integration.test.ts tests/integration/ServerModeRoot.test.ts tests/integration/notification-subscription.test.ts tests/integration/SignalHandler.integration.test.ts tests/integration/FrpTunnel.integration.test.ts tests/integration/DockerCluster.integration.test.ts tests/integration/EdgeClusterFlow.test.ts tests/integration/MultiNodeCluster.integration.test.ts",
    "test:docker:all": "yarn test:docker:lite && yarn test:docker:full",
    "test:docker:lite:up": "docker-compose -f docker-compose.standalone.yml up -d --build",
    "test:docker:lite:down": "docker-compose -f docker-compose.standalone.yml down",
    "test:service": "XPOD_RUN_SERVICE_TESTS=true vitest --run tests/service",
    "test:setup": "dotenv -e .env.local -- yarn ts-node scripts/setup-test-credentials.ts",
    "test:setup:docker": "CSS_BASE_URL=${CSS_BASE_URL:-http://localhost:5739} yarn ts-node scripts/setup-test-credentials.ts",
    "build:single": "node scripts/build-single-file.cjs",
    "pack:single": "yarn build:single && npm pack --json",
    "publish:local": "node scripts/publish-local.cjs",
    "publish:local:dry": "XPOD_PUBLISH_DRY_RUN=true node scripts/publish-local.cjs --dry-run"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@types/better-sqlite3": "^7.6.13",
    "@types/http-proxy": "^1.17.17",
    "@types/nat-upnp": "^1.1.5",
    "@types/node": "^22.0.0",
    "@types/pg": "^8.10.2",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.4.0",
    "autoprefixer": "^10.4.17",
    "componentsjs-generator": "^4.3.0",
    "dotenv": "^16.4.5",
    "dotenv-cli": "^7.4.4",
    "drizzle-kit": "^0.31.8",
    "jsdom": "^24.0.0",
    "jsonld": "^8.3.3",
    "memory-level": "^3.1.0",
    "nat-upnp": "^1.1.1",
    "node-pty": "^1.1.0",
    "nodemon": "^3.1.10",
    "postcss": "^8.4.35",
    "rdf-test-suite": "^2.1.3",
    "tailwindcss": "^3.4.3",
    "ts-node": "^10.9.2",
    "typescript": "5.5",
    "vite": "^5.1.6",
    "vitest": "^1.4.0"
  },
  "dependencies": {
    "@ai-sdk/openai": "^3.0.2",
    "@comunica/query-sparql-rdfjs": "^4.0.2",
    "@electric-sql/pglite": "^0.3.14",
    "@inrupt/solid-client-authn-node": "^3.1.0",
    "@solid/community-server": "^8.0.0-alpha.1",
    "abstract-level": "^1.0.4",
    "acme-client": "^5.1.3",
    "ai": "^6.0.6",
    "asynciterator": "^3.9.0",
    "awilix": "^12.0.5",
    "better-sqlite3": "^12.5.0",
    "classic-level": "^1.4.1",
    "clsx": "^2.1.0",
    "drizzle-orm": "^0.30.10",
    "drizzle-solid": "^0.2.5",
    "http-proxy": "^1.18.1",
    "i18next": "^23.10.1",
    "knex": "^3.1.0",
    "level-supports": "^4.0.1",
    "minio": "^8.0.2",
    "pg": "^8.13.1",
    "pg-query-stream": "^4.7.1",
    "pgvector": "^0.2.1",
    "portfinder": "^1.0.38",
    "quadstore": "^15.4.1",
    "quadstore-comunica": "^6.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^13.3.1",
    "react-router-dom": "^6.22.3",
    "sqlite-vec": "^0.1.7-alpha.2",
    "sqlite3": "^5.1.7",
    "tree-kill": "^1.2.2",
    "werift": "^0.22.2",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "yargs": "^17.7.2",
    "zod": "^3.25.76"
  },
  "resolutions": {
    "@inrupt/solid-client-authn-browser": "^3.1.1",
    "@inrupt/solid-client-authn-core": "^3.1.1",
    "@comunica/query-sparql-solid": "^4.0.2",
    "@comunica/query-sparql-rdfjs": "^4.0.2",
    "@comunica/query-sparql": "^4.5.0",
    "@comunica/core": "^4.5.0",
    "@comunica/types": "^4.5.0",
    "@comunica/context-entries": "^4.5.0",
    "@comunica/mediator-number": "^4.5.0",
    "@comunica/mediator-all": "^4.5.0",
    "@comunica/mediator-race": "^4.5.0",
    "@comunica/mediator-combine-union": "^4.5.0",
    "@comunica/mediator-combine-pipeline": "^4.5.0",
    "componentsjs": "^6.4.0"
  }
}
