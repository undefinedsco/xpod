{
  "@context": [
    "https://linkedsoftwaredependencies.org/bundles/npm/@solid/community-server/^8.0.0/components/context.jsonld",
    "https://linkedsoftwaredependencies.org/bundles/npm/@undefineds/xpod/^0.0.0/components/context.jsonld"
  ],
  "import": [
    "./extensions.server.router.json",
    "./xpod.cluster.router.json"
  ],
  "@graph": [
    {
      "comment": "Override SignalAwareHttpHandler to use RouterHttpHandler for /api/v1/signal.",
      "@type": "Override",
      "overrideInstance": {
        "@id": "urn:undefineds:xpod:SignalAwareHttpHandler"
      },
      "overrideParameters": {
        "@type": "RouterHttpHandler",
        "routes": [
          {
            "@type": "RouterHttpRoute",
            "basePath": "/api/v1/signal",
            "handler": { "@id": "urn:undefineds:xpod:EdgeNodeSignalHttpHandler" }
          }
        ],
        "fallback": {
          "@id": "urn:solid-server:default:HttpHandler"
        }
      }
    }
  ]
}
