{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "lib": [
      "es2021",
      "dom",
      "dom.iterable"
    ],
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "jsx": "react-jsx",
    "types": [
      "vite/client"
    ],
    "baseUrl": "./ui/admin/src",
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "ui/admin/src/**/*",
    "ui/admin/index.html"
  ]
}
